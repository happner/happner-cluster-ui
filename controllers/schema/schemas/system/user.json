{
  "title":"user",
  "properties":{
    "name": {
      "id": "/properties/name",
      "type": "string"
    },
    "number": {
      "id": "/properties/number",
      "type": "integer"
    },
    "organisation": {
      "id": "/properties/organisation",
      "type": "integer"
    },
    "deleted": {
      "type":"string",
      "format":"datetime-local",
      "readOnly":true
    },
    "_meta": {
      "id": "/properties/_meta",
      "properties": {
        "created": {
          "type":"string",
          "format":"datetime-local",
          "readOnly":true
        },
        "createdBy": {
          "id": "/properties/_meta/properties/createdBy",
          "type": "string"
        },
        "modified": {
          "type":"string",
          "format":"datetime-local",
          "readOnly":true
        }
      },
      "type": "object"
    },
    "password":{
      "type":"string",
      "format":"password"
    },
    "publicKey":{
      "type":"string"
    },
    "groups":{
      "type":"array",
      "uniqueItems": true,
      "items": {
        "type": "object",
        "#ref": "Group"
      }
    },
    "status":{
      "type":"string",
      "enum":["active","inactive"],
      "required":true
    }
  }
}
